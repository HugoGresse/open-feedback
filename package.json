{
    "name": "open-feedback",
    "version": "0.16.0",
    "private": true,
    "homepage": "https://openfeedback.io/",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "deploy": "npm run build && firebase deploy",
        "release": "npm version minor && npm run deploy && git push && git push --tags",
        "doc": "start-storybook -p 6006 --docs",
        "doc:build": "build-storybook --docs",
        "cy:open": "env-cmd -f .env.test.local cypress open",
        "cy:run": "cypress run",
        "cy:start": "env-cmd -f .env.test.local react-scripts start",
        "test:cy": "npm run start:dist & wait-on http://localhost:3000 && npm run cy:run",
        "test:cy:stop": "kill -9 $(lsof -i :3000 | grep 'node' | awk '{print $2}')",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public"
    },
    "devDependencies": {
        "@storybook/addon-docs": "^5.3.12",
        "@storybook/addons": "^5.3.10",
        "@storybook/preset-create-react-app": "^1.5.2",
        "@storybook/react": "5.3.10",
        "babel-loader": "^8.0.6",
        "cypress": "^3.8.3",
        "eslint-plugin-cypress": "^2.8.1",
        "firebase-tools": "^7.12.1",
        "husky": "^4.2.1",
        "lint-staged": "^10.0.7",
        "prettier": "^1.19.1",
        "react-scripts": "3.3.1"
    },
    "dependencies": {
        "@date-io/luxon": "^1.3.13",
        "@material-ui/core": "^4.9.1",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/lab": "^4.0.0-alpha.39",
        "@material-ui/pickers": "^3.2.10",
        "clipboard-copy": "^3.1.0",
        "env-cmd": "^10.0.1",
        "file-saver": "^2.0.2",
        "firebase": "^7.8.0",
        "formik": "^2.1.3",
        "history": "^4.10.1",
        "i18next": "^19.1.0",
        "i18next-browser-languagedetector": "^4.0.1",
        "i18next-scanner": "^2.10.3",
        "lodash": "4.17.15",
        "luxon": "^1.21.3",
        "polished": "^3.4.4",
        "prop-types": "latest",
        "qrcode.react": "^1.0.0",
        "react": "^16.12.0",
        "react-color": "^2.18.0",
        "react-dom": "^16.12.0",
        "react-firebaseui": "^4.1.0",
        "react-ga": "^2.7.0",
        "react-i18next": "^11.3.1",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "recharts": "^1.8.5",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-components": "^5.0.1",
        "styled-system": "^5.1.4",
        "wait-on": "^3.3.0",
        "yup": "^0.28.1"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "eslintIgnore": [
        "*.ts"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
