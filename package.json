{
    "name": "open-feedback",
    "version": "0.0.1",
    "private": true,
    "homepage": "https://openfeedback.io/",
    "devDependencies": {
        "babel-loader": "8.0.6",
        "cypress": "^3.4.1",
        "eslint-plugin-cypress": "^2.6.1",
        "firebase-tools": "^7.3.2",
        "husky": "^3.0.5",
        "lint-staged": "^9.3.0",
        "prettier": "^1.18.2",
        "react-scripts": "3.1.2"
    },
    "dependencies": {
        "@material-ui/core": "^4.4.2",
        "@material-ui/icons": "^4.4.1",
        "docz": "^1.2.0",
        "docz-theme-default": "^1.2.0",
        "firebase": "^6.6.2",
        "formik": "^1.5.8",
        "history": "^4.10.1",
        "lodash": "^4.17.15",
        "moment": "^2.23.0",
        "polished": "^3.4.1",
        "prop-types": "latest",
        "react": "^16.9.0",
        "react-color": "^2.17.3",
        "react-dom": "^16.9.0",
        "react-firebaseui": "^4.0.0",
        "react-ga": "^2.6.0",
        "react-redux": "^7.1.1",
        "react-router-dom": "^5.0.1",
        "recharts": "^1.7.1",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-components": "^4.3.2",
        "styled-system": "^5.1.2",
        "wait-on": "^3.3.0",
        "yup": "^0.27.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "deploy": "npm run build && firebase deploy",
        "docz:dev": "docz dev",
        "docz:build": "docz build",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "test:cy": "npm run start:dist & wait-on http://localhost:3000 && npm run cy:run",
        "test:cy:stop": "kill -9 $(lsof -i :3000 | grep 'node' | awk '{print $2}')"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
