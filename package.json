{
    "name": "open-feedback",
    "version": "0.22.0",
    "private": true,
    "homepage": "https://openfeedback.io/",
    "scripts": {
        "start": "react-scripts start",
        "start:prod": "env-cmd -f .env.production.local react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "deploy": "npm run build && firebase deploy",
        "release": "npm version minor && npm run deploy && git push && git push --tags",
        "doc": "start-storybook -p 6006 --docs",
        "doc:build": "build-storybook --docs",
        "cy:open": "env-cmd -f .env.test.local cypress open",
        "cy:run": "cypress run",
        "cy:start": "env-cmd -f .env.test.local react-scripts start",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public",
        "lint:i18n": "eslint src/**/languages/*.json"
    },
    "devDependencies": {
        "@storybook/addon-docs": "^6.1.11",
        "@storybook/addons": "^6.1.11",
        "@storybook/preset-create-react-app": "^3.1.5",
        "@storybook/react": "6.1.11",
        "cypress": "^4.6.0",
        "eslint-plugin-cypress": "^2.11.2",
        "eslint-plugin-i18n-json": "^3.1.0",
        "firebase-tools": "^9.0.1",
        "husky": "^4.3.6",
        "jest-fetch-mock": "^3.0.3",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1"
    },
    "dependencies": {
        "@date-io/luxon": "^1.3.13",
        "@material-ui/core": "^4.11.2",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "@material-ui/pickers": "^3.2.10",
        "@material-ui/utils": "^4.11.2",
        "@nivo/core": "^0.67.0",
        "@nivo/line": "^0.67.0",
        "clipboard-copy": "^4.0.1",
        "env-cmd": "^10.1.0",
        "file-saver": "^2.0.5",
        "firebase": "^7.14.5",
        "formik": "^2.2.6",
        "history": "^5.0.0",
        "i18next": "^19.8.4",
        "i18next-browser-languagedetector": "^6.0.1",
        "langmap": "0.0.16",
        "lodash": "4.17.20",
        "luxon": "^1.25.0",
        "prop-types": "latest",
        "qrcode.react": "^1.0.0",
        "react": "^17.0.1",
        "react-color": "^2.19.3",
        "react-dom": "^17.0.1",
        "react-dropzone": "^11.2.4",
        "react-firebaseui": "^4.1.0",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.8.5",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.1",
        "react-scrollable-anchor": "^0.6.1",
        "react-transition-group": "^4.4.1",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-components": "^5.2.1",
        "styled-system": "^5.1.5",
        "wait-on": "^5.2.0",
        "yup": "^0.32.8"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "eslintIgnore": [
        "*.ts"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js,json,css,md}": [
            "prettier --write"
        ]
    }
}
