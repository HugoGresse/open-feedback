{
    "name": "open-feedback",
    "version": "0.27.1",
    "private": true,
    "homepage": "https://openfeedback.io/",
    "type": "module",
    "scripts": {
        "start": "vite",
        "start:prod": "vite --mode production",
        "start:local": "firebase emulators:start --import=./cypress/fixtures/firebase-emulators-data --only firestore,auth,functions,storage & npm run start",
        "start:local:back": "firebase emulators:start --import=./cypress/fixtures/firebase-emulators-data --only firestore,auth,functions,storage",
        "build": "vite build",
        "build:dev": "vite build --mode development",
        "test": "firebase emulators:exec --only firestore vitest",
        "deploy": "npm run build && firebase deploy",
        "deploy:dev": "npm run build:dev && firebase deploy",
        "release": "npm version minor && npm run deploy && git push && git push --tags",
        "doc": "start-storybook -p 6006 --docs",
        "doc:build": "build-storybook",
        "cy:open": "env-cmd -f .env.test.local cypress open",
        "cy:run": "cypress run",
        "cy:start": "env-cmd -f .env.test.local npm run start:local",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public",
        "lint:i18n": "eslint src/**/languages/*.json",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@firebase/testing": "^0.20.11",
        "@storybook/addon-docs": "^8.0.4",
        "@storybook/addons": "^7.6.17",
        "@storybook/react": "^8.0.4",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react": "^4.2.1",
        "axe-core": "^4.3.2",
        "cypress": "13.7.1",
        "cypress-axe": "^1.5.0",
        "cypress-vite": "^1.5.0",
        "eslint": "^8.57.0",
        "eslint-plugin-cypress": "^3.2.0",
        "eslint-plugin-i18n-json": "^4.0.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "firebase-tools": "^13.9.0",
        "husky": "^5.1.3",
        "jest-fetch-mock": "^3.0.3",
        "lint-staged": "^10.5.4",
        "prettier": "^3.2.5",
        "typescript": "^5.2.2",
        "vite": "^5.2.0",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "@emotion/styled": "^11.11.0",
        "@mdi/js": "^7.3.67",
        "@mdi/react": "^1.6.1",
        "@mui/icons-material": "^5.15.14",
        "@mui/lab": "5.0.0-alpha.168",
        "@mui/material": "^5.15.14",
        "@mui/styles": "^5.15.14",
        "@mui/x-date-pickers": "^7.4.0",
        "@nivo/core": "^0.85.1",
        "@nivo/line": "^0.85.1",
        "clipboard-copy": "^4.0.1",
        "env-cmd": "^10.1.0",
        "file-saver": "^2.0.5",
        "firebase": "^10.9.0",
        "firebaseui": "^6.1.0",
        "formik": "^2.2.6",
        "history": "^5.0.0",
        "i18next": "^19.9.2",
        "i18next-browser-languagedetector": "^6.1.1",
        "langmap": "0.0.16",
        "lodash": "4.17.21",
        "luxon": "3.4.4",
        "prop-types": "latest",
        "qrcode.react": "^3.1.0",
        "react": "^18.2.0",
        "react-color": "^2.19.3",
        "react-dom": "^18.2.0",
        "react-dropzone": "^11.3.2",
        "react-google-recaptcha-v3": "^1.8.0",
        "react-helmet-async": "^1.2.2",
        "react-hotkeys-hook": "^3.3.1",
        "react-i18next": "^11.14.2",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.22.3",
        "react-spring": "^8.0.27",
        "react-transition-group": "^4.4.2",
        "redux": "^5.0.1",
        "redux-thunk": "^3.1.0",
        "reselect": "^5.1.0",
        "styled-components": "^6.1.8",
        "styled-system": "^5.1.5",
        "wait-on": "^7.2.0",
        "yup": "^0.32.9"
    },
    "overrides": {
        "@mui/styles": {
            "react": "react"
        }
    },
    "eslintIgnore": [
        "*.ts"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js,json,css,md}": [
            "prettier --write"
        ]
    }
}
