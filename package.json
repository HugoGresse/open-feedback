{
    "name": "open-feedback",
    "version": "0.10.0",
    "private": true,
    "homepage": "https://openfeedback.io/",
    "devDependencies": {
        "babel-loader": "8.0.6",
        "cypress": "^3.6.1",
        "eslint-plugin-cypress": "^2.7.0",
        "firebase-tools": "^7.7.0",
        "husky": "^3.1.0",
        "lint-staged": "^9.4.3",
        "prettier": "^1.19.1",
        "react-scripts": "3.2.0"
    },
    "dependencies": {
        "@date-io/moment": "^1.3.11",
        "@material-ui/core": "^4.6.1",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/pickers": "^3.2.8",
        "clipboard-copy": "^3.1.0",
        "docz": "^1.2.0",
        "docz-theme-default": "^1.2.0",
        "env-cmd": "^10.0.1",
        "firebase": "^7.4.0",
        "formik": "^2.0.4",
        "history": "^4.10.1",
        "lodash": "4.17.15",
        "moment": "^2.24.0",
        "polished": "^3.4.2",
        "prop-types": "latest",
        "react": "^16.11.0",
        "react-color": "^2.17.3",
        "react-dom": "^16.11.0",
        "react-firebaseui": "^4.0.0",
        "react-ga": "^2.7.0",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "recharts": "^1.8.5",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-components": "^4.4.1",
        "styled-system": "^5.1.2",
        "wait-on": "^3.3.0",
        "yup": "^0.27.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "deploy": "npm run build && firebase deploy",
        "release": "npm version minor && npm run deploy && git push && git push --tags",
        "docz:dev": "docz dev",
        "docz:build": "docz build",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "cy:start": "env-cmd -f .env.test.local react-scripts start",
        "test:cy": "npm run start:dist & wait-on http://localhost:3000 && npm run cy:run",
        "test:cy:stop": "kill -9 $(lsof -i :3000 | grep 'node' | awk '{print $2}')"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
